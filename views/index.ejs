  <script>
    // Get references to elements
    const toggleViewButton = document.getElementById('toggle-view-button');
    const sliderContainer = document.getElementById('slider-container');
    
    // Initialize state (slider view by default)
    let isSliderView = true;
  
    // Toggle view function
    toggleViewButton.addEventListener('click', () => {
      if (isSliderView) {
        // Switch to grid layout
        sliderContainer.classList.remove('flex', 'overflow-x-scroll', 'space-x-6'); // Remove slider styles
        sliderContainer.classList.add('grid', 'grid-cols-1', 'sm:grid-cols-2', 'md:grid-cols-3', 'lg:grid-cols-4', 'gap-6'); // Add grid styles
        toggleViewButton.textContent = 'Back to Slider';  // Change button text
      } else {
        // Switch back to slider layout
        sliderContainer.classList.add('flex', 'overflow-x-scroll', 'space-x-6'); // Add slider styles back
        sliderContainer.classList.remove('grid', 'grid-cols-1', 'sm:grid-cols-2', 'md:grid-cols-3', 'lg:grid-cols-4', 'gap-6'); // Remove grid styles
        toggleViewButton.textContent = 'View All Products';  // Reset button text
      }
  
      // Toggle state
      isSliderView = !isSliderView;
    });
  </script>


